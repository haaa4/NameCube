<ui:FluentWindow
    x:Class="NameCube.ToolBox.AutomaticProcessPages.ProcessesRunningWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:NameCube.ToolBox.AutomaticProcessPages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
    Title="自动流程"
    Width="600"
    Height="400"
    mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50" />
            <RowDefinition />
        </Grid.RowDefinitions>
        <ui:TitleBar
            x:Name="MainTitle"
            Title="自动流程"
            Icon="{ui:ImageIcon 'pack://application:,,,/icon.ico'}"
            ShowHelp="False"
            ShowMaximize="False"
            ShowMinimize="False" />
        <Frame
            x:Name="MainFrame"
            Grid.Row="1"
            NavigationUIVisibility="Hidden" />
    </Grid>
</ui:FluentWindow>
